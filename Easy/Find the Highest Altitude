class Solution {
    public int largestAltitude(int[] gain) {
        int[] op=new int [gain.length+1];
        op[0]=0;
        int max=0;
        for (int i=1;i<gain.length;i++){
            op[i]=op[i-1]+gain[i-1];
            if(op[i]>max){
                max=op[i];
            }
        }
        return max;
        
    }
}
