class Solution {
    public int maximumWealth(int[][] accounts) {
        int maxi=0;
        for(int[] i: accounts){
            int sumi=0;
            for(int j=0;j<i.length;j++){
                sumi=sumi+i[j];
            }
            if(sumi>maxi){
                maxi=sumi;
            }
        }
        
         return maxi;
        
    }
}
